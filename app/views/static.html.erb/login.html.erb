<h1>Login</h1>
  <%=form_for @user url: user_path(@user) do |f|%>
    <%=f.label "Username"%>
    <%=f.text_field :username%>
    <%<%=f.label "Password"%>
    <%=f.text_field :password%>
  <%end%>
<h1>Create Account</h1>
  <%=form_for @user url: new_user_path(@user) do |f|%>
    <%=f.label "Username"%>
    <%=f.text_field :username%>
    <%<%=f.label "Password"%>
    <%=f.text_field :password%>
  <%end%>
